<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <!-- Font Awesome -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css" rel="stylesheet" />
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700&display=swap" rel="stylesheet" />
    <!-- MDB -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/6.2.0/mdb.min.css" rel="stylesheet" />
    <style>
        .card {
            box-shadow: 0 0 15px -2px #444444;
            ;

        }

        .main-heading {
            color: #1c4980;
        }
    </style>
</head>

<body>
    <section class="h-100 bg-light">
        <div class="container py-5 h-100">
            <div class="row d-flex justify-content-center align-items-center h-100">
                <div class="col">
                    <div class="card card-registration my-4">
                        <div class="row g-0">

                            <div class="col-xl-12">
                                <% if(typeof message !="undefined" && message){ %>
                                    <div class="alert alert-success alert-dismissible fade show" role="alert">
                                        <%= message.message %>
                                            <button type="button" class="btn-close" data-bs-dismiss="alert"
                                                aria-label="Close"></button>
                                    </div>
                                    <% } %>
                                        <div class="card-body p-md-5 text-black">
                                            <h3 class="mb-5 text-uppercase main-heading">Upload Project Repository
                                                Details</h3>

                                            <form action="/uploadProjectRepository" method="post"
                                                enctype="multipart/form-data" id="addProjectRepositoryForm" class="addProjectRepositoryForm">
                                                <div class="form-outline mb-4">
                                                    <input type="text" id="projectName" name="projectName"
                                                        class="form-control form-control-lg" required />
                                                        <div class="invalid-feedback">
                                                            Please provide a project anme
                                                          </div>
                                                    <label class="form-label" for="projectName">projectName</label>
                                                </div>

                                                <div class="form-outline mb-4">
                                                    <input type="text" id="teamMembers" name="teamMembers"
                                                        class="form-control  form-control-lg" required />
                                                    <label class="form-label" for="teamMembers">teamMembers</label>

                                                </div>

                                                <div class="form-outline mb-4">
                                                    <input type="text" id="guideName" name="guideName"
                                                        class="form-control  form-control-lg" required />
                                                    <label class="form-label" for="guideName">guideName</label>

                                                </div>

                                                <div class="form-outline mb-4">
                                                    <textarea type="input" class="form-control  form-control-lg"
                                                        id="projectDescription" name="projectDescription" rows="3"
                                                        required></textarea>
                                                    <label class="form-label"
                                                        for="projectDescription">projectDescription</label>
                                                </div>

                                                <div class="form-outline mb-4">
                                                    <textarea type="input" class="form-control  form-control-lg"
                                                        id="objective" name="objective" rows="3" required></textarea>
                                                    <label class="form-label" for="objective">objective</label>
                                                </div>


                                                <div class="row">
                                                    <div class="col-md-6 mb-4">

                                                        <select class="form-select form-select-lg" id="collegeName"
                                                            name="collegeName" aria-label="Default select example"
                                                            required>
                                                            <option value="" selected>Choose collegeName</option>
                                                        </select>

                                                    </div>
                                                    <div class="col-md-6 mb-4">
                                                        <select class="form-select form-select-lg" id="universityName"
                                                            name="universityName" aria-label="Default select example"
                                                            required>
                                                            <option value="" selected>Choose universityName</option>

                                                        </select>

                                                    </div>
                                                </div>



                                                <div class="form-outline mb-4">
                                                    <input type="number" pattern="[0-9]{10}" id="phone" name="phone"
                                                        class="form-control form-control-lg" required />
                                                    <label class="form-label" for="phone">phone</label>
                                                </div>

                                                <div class="form-outline mb-4">
                                                    <input type="email" id="email" name="email"
                                                        class="form-control form-control-lg" required />
                                                    <label class="form-label" for="email">email</label>
                                                </div>

                                                <div class="form-outline mb-4">
                                                    <input type="text" id="skillsToBeLearned" name="skillsToBeLearned"
                                                        class="form-control form-control-lg" required />
                                                    <label class="form-label" for="skillsToBeLearned">Skills
                                                        Required(Seperate By
                                                        Comma(,) )</label>
                                                </div>
                                                <div class="form-outline mb-4">
                                                    <input type="text" id="technologiesUsed" name="technologiesUsed"
                                                        class="form-control form-control-lg" required />
                                                    <label class="form-label"
                                                        for="technologiesUsed">technologiesUsed(Seperate By
                                                        Comma(,) )</label>
                                                </div>

                                                <div class="row">
                                                    <div class="col-md-6 mb-4">

                                                        <div class="form-outline">
                                                            <input type="text" id="projectSourceCode"
                                                                name="projectSourceCode"
                                                                class="form-control form-control-lg" required />
                                                            <label class="form-label"
                                                                for="projectSourceCode">projectSourceCode(link)</label>
                                                        </div>

                                                    </div>
                                                    <div class="col-md-6 mb-4">

                                                        <div class="form-outline">
                                                            <input type="text" id="videoDemo" name="videoDemo"
                                                                class="form-control form-control-lg" required />
                                                            <label class="form-label"
                                                                for="videoDemo">videoDemo(link)</label>
                                                        </div>

                                                    </div>
                                                </div>




                                                <div class=" mb-4">
                                                    <label class="form-label" for="projectReport">projectReport</label>

                                                    <input type="file" id="projectReport" name="projectReport"
                                                        class="form-control  form-control-lg" required
                                                        accept="application/pdf" />

                                                </div>
                                                <div class=" mb-4">
                                                    <label class="form-label" for="projectImage">projectImage</label>

                                                    <input type="file" id="projectImage" name="projectImage"
                                                        class="form-control  form-control-lg" required
                                                        accept="image/jpg,image/png" />
                                                </div>

                                                <div class="d-flex justify-content-center pt-3">
                                                    <input id="submitProjectRepoForm" type="submit"
                                                        class="btn btn-primary ms-2 btn-lg" value="Submit">
                                                </div>
                                            </form>




                                        </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
</body>
<!-- MDB -->
<!-- jquery cdnjs -->
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/mdb-ui-kit/6.2.0/mdb.min.js"></script>
<script type="text/javascript" src="/js/ProjectRepository/uploadProjectRepository.js"></script>

</html>