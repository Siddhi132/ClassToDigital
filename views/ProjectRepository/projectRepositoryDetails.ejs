<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>projectRepository details</title>
    <style>
        body {
            background-color: #f6f9ff !important;
        }

        .min-height-50 {
            min-height: 75px;
        }

        .min-height-100 {
            min-height: 120px;
        }

        .project-title {
            font-size: 25px;

            .small-text {
                font-size: 12px;
            }

            .ownbadge {
                width: 300px !important;

                background-color: #dfe7f6;

                color: #2c58a0;

            }

        }

        @media only screen and (max-width: 768px) {
            .project-title {
                font-size: 16px;

            }

            .small-text {
                font-size: 14px;
            }

        }
    </style>
</head>

<body>

    <% if(login){ %>
        <% if(typeof role!="undefined" && role){ %>
            <% if(role=="student" ){ %>
                <%- include('../Navbar/StudentNavbar.ejs') %>
                    <% }else if(role=="mentor" ){ %>
                        <%- include('../Navbar/MentorNavbar.ejs') %>
                            <% }else if(role=="admin" ){ %>
                                <%- include('../Navbar/AdminNavbar.ejs') %>
                                    <% }else if(role=="company" ){ %>
                                        <%- include('../Navbar/CompanyNavbar.ejs') %>
                                            <% } %>
                                                <% } %>

                                                    <% }else { %>
                                                        <%- include('../Navbar/InitNavbar.ejs') %>
                                                            <% } %>



                                                                <% projectRepository.forEach(function(repository){ %>

                                                                    <% if(repository.isHidden){ %>
                                                                        <div class="container mt-4 mb-5">
                                                                            <div class="row">
                                                                                <div class="col-md-12">
                                                                                    <p
                                                                                        class="mb-2 text-center text-black">
                                                                                        Opps! Project Repository is
                                                                                        Hidden By owner</p>
                                                                                </div>
                                                                            </div>
                                                                        </div>
                                                                        <% return; %>
                                                                            <% } %>

                                                                                <main
                                                                                    class="card p-sm-5 pt-sm-2 pb-sm-5 mb-sm-4 mt-sm-2 p-3 m-3 text-start text-sm-start">
                                                                                    <span>
                                                                                        <p
                                                                                            class="text-center project-title">
                                                                                            Project Name : <strong>
                                                                                                <%= repository.projectName
                                                                                                    %>
                                                                                            </strong> </p>
                                                                                    </span>
                                                                                    <div
                                                                                        class="container  border-primary ">

                                                                                        <div class="row border-dark">


                                                                                            <div
                                                                                                class="col-md-5 mb-4 bg-image p-sm-4 pt-sm-2">
                                                                                                <img src="<%= repository.projectImage.path %>"
                                                                                                    class="img-fluid "
                                                                                                    alt="" />


                                                                                                <div
                                                                                                    class="d-sm-flex flex-column text-start justify-content-between small-text card ripple">
                                                                                                    <div
                                                                                                        class="card-footer text-muted">
                                                                                                        College
                                                                                                        :<strong>
                                                                                                            <%=
                                                                                                                repository.collegeName%>
                                                                                                        </strong> ,
                                                                                                        University :
                                                                                                        <strong>
                                                                                                            <%= repository.universityName
                                                                                                                %>
                                                                                                        </strong>
                                                                                                    </div>
                                                                                                    <div
                                                                                                        class="card-footer text-muted">
                                                                                                        Academic Mentor
                                                                                                        :<strong>
                                                                                                            <%= repository.IndustryMentorName
                                                                                                                %>
                                                                                                        </strong> ,
                                                                                                        Industry Mentor
                                                                                                        : <strong>
                                                                                                            <%= repository.universityName
                                                                                                                %>
                                                                                                        </strong></div>
                                                                                                    <div
                                                                                                        class="card-footer text-muted">
                                                                                                        City :<strong>
                                                                                                            <%=
                                                                                                                repository.city%>
                                                                                                        </strong>,State
                                                                                                        :<strong>
                                                                                                            <%= repository.state
                                                                                                                %>
                                                                                                        </strong> </div>
                                                                                                    <div
                                                                                                        class="card-footer text-muted">
                                                                                                        Project StatDate
                                                                                                        : <% if
                                                                                                            (repository.projectstartDate)
                                                                                                            { %>
                                                                                                            <% const
                                                                                                                startDate=new
                                                                                                                Date(repository.projectstartDate);
                                                                                                                %>
                                                                                                                <strong>
                                                                                                                    <%= startDate.getDate()
                                                                                                                        %>
                                                                                                                        <%= startDate.toLocaleString('default',
                                                                                                                            {
                                                                                                                            month: 'long'
                                                                                                                            })
                                                                                                                            %>
                                                                                                                            <%= startDate.getFullYear()
                                                                                                                                %>
                                                                                                                </strong>
                                                                                                                <% } else
                                                                                                                    { %>
                                                                                                                    <strong>-</strong>
                                                                                                                    <% }
                                                                                                                        %>
                                                                                                                        </strong>
                                                                                                    </div>
                                                                                                    <div
                                                                                                        class="card-footer text-muted">
                                                                                                        Project EndDate
                                                                                                        : <% if
                                                                                                            (repository.projectendDate)
                                                                                                            { %>
                                                                                                            <% const
                                                                                                                endDate=new
                                                                                                                Date(repository.projectendDate);
                                                                                                                %>
                                                                                                                <strong>
                                                                                                                    <%= endDate.getDate()
                                                                                                                        %>
                                                                                                                        <%= endDate.toLocaleString('default',
                                                                                                                            {
                                                                                                                            month: 'long'
                                                                                                                            })
                                                                                                                            %>
                                                                                                                            <%= endDate.getFullYear()
                                                                                                                                %>
                                                                                                                </strong>
                                                                                                                <% } else
                                                                                                                    { %>
                                                                                                                    <strong>-</strong>
                                                                                                                    <% }
                                                                                                                        %>
                                                                                                                        </strong>
                                                                                                    </div>


                                                                                                </div>
                                                                                            </div>




                                                                                            <div
                                                                                                class="col-md-7 mb-4 overflow-hidden text-center">


                                                                                                <div
                                                                                                    class="p-0 p-sm-4 small-text text-start">
                                                                                                    <div class="mb-3">
                                                                                                        Type Of Project
                                                                                                        :
                                                                                                        <a href="">
                                                                                                            <span
                                                                                                                class="badge badge-secondary">
                                                                                                                <%= repository.typeOfProject
                                                                                                                    %>
                                                                                                            </span>
                                                                                                        </a>

                                                                                                    </div>

                                                                                                    <p
                                                                                                        class="card-text my-2 min-height-50 ">
                                                                                                        <strong>Project
                                                                                                            Domain
                                                                                                            :</strong>
                                                                                                        <%= repository.projectDomain
                                                                                                            %>
                                                                                                    </p>
                                                                                                    <div
                                                                                                        class="mb-3 d-flex flex-sm-row flex-column  sm:mb-3 mt-5">
                                                                                                        <span
                                                                                                            class="label label-dark ">Team
                                                                                                            Members
                                                                                                            :</span>

                                                                                                        <% var
                                                                                                            members=repository.teamMember.split(',');
                                                                                                            %>
                                                                                                            <% members.forEach(function(member){
                                                                                                                %>
                                                                                                                <div>

                                                                                                                    <a
                                                                                                                        href="">
                                                                                                                        <span
                                                                                                                            class="badge badge-primary mx-2 ">
                                                                                                                            <%= member
                                                                                                                                %>
                                                                                                                        </span>
                                                                                                                    </a>

                                                                                                                </div>
                                                                                                                <% }) %>


                                                                                                    </div>



                                                                                                    <div
                                                                                                        class="row d-flex flex-column justify-content-center text-center mb-4 me-5 ms-5">

                                                                                                        <% if
                                                                                                            (repository.linkforAccessCode)
                                                                                                            { %>
                                                                                                            <a class="btn btn-primary ms-1  m-3"
                                                                                                                href="<%= repository.linkforAccessCode%>"
                                                                                                                target="_blank">
                                                                                                                Access
                                                                                                                Code
                                                                                                                <i
                                                                                                                    class="fas fa-link ms-1 "></i>
                                                                                                            </a>
                                                                                                            <% } %>
                                                                                                                <% if
                                                                                                                    (repository.linkforAccessPrototype)
                                                                                                                    { %>
                                                                                                                    <a class="btn btn-primary ms-1 m-3"
                                                                                                                        href="<%= repository.linkforAccessPrototype %>"
                                                                                                                        target="_blank">
                                                                                                                        Link
                                                                                                                        for
                                                                                                                        Prototype
                                                                                                                        <i
                                                                                                                            class="fas fa-link ms-1 "></i>
                                                                                                                    </a>
                                                                                                                    <% }
                                                                                                                        %>
                                                                                                                        <% if
                                                                                                                            (repository.projectReport.path)
                                                                                                                            {
                                                                                                                            %>
                                                                                                                            <button
                                                                                                                                class="btn btn-primary ms-1  m-3">
                                                                                                                                <a class="text-white"
                                                                                                                                    href="<%= repository.projectReport.path %>"
                                                                                                                                    target="_blank">Project
                                                                                                                                    Report</a>
                                                                                                                                <i
                                                                                                                                    class="fas fa-download ms-1 "></i>
                                                                                                                                <% }
                                                                                                                                    %>
                                                                                                                            </button>
                                                                                                    </div>


                                                                                                </div>

                                                                                            </div>

                                                                                        </div>
                                                                                        <div
                                                                                            class=" border-success p-0 m-0 p-sm-5 m-sm-5 pt-sm-0 mt-sm-0">





                                                                                            <div
                                                                                                class="row d-flex justify-content-center">
                                                                                                <p
                                                                                                    class="card-text my-2 min-height-100">
                                                                                                    <strong>Brief
                                                                                                        Description
                                                                                                        :</strong>
                                                                                                    <%= repository.briefDescription
                                                                                                        %>
                                                                                                </p>


                                                                                                <p
                                                                                                    class="card-text my-2 min-height-50">
                                                                                                    <strong>Problems
                                                                                                        Define
                                                                                                        :</strong>
                                                                                                    <%= repository.problemsDefine
                                                                                                        %>
                                                                                                </p>

                                                                                                <p
                                                                                                    class="card-text my-2 min-height-50">
                                                                                                    <strong>Project
                                                                                                        Solution
                                                                                                        :</strong>
                                                                                                    <%= repository.problemsSolution
                                                                                                        %>
                                                                                                </p>

                                                                                                <p
                                                                                                    class="card-text my-2 min-height-50">
                                                                                                    <strong>Working Of
                                                                                                        Project
                                                                                                        :</strong>
                                                                                                    <%= repository.workingOfProject
                                                                                                        %>
                                                                                                </p>
                                                                                                <p
                                                                                                    class="card-text my-2 min-height-50">
                                                                                                    <strong>USP
                                                                                                        regarding
                                                                                                        project :
                                                                                                        :</strong>
                                                                                                    <%= repository.usp
                                                                                                        %>
                                                                                                </p>
                                                                                                <div class="mb-3">
                                                                                                    <strong
                                                                                                        class="label label-dark">
                                                                                                        Application
                                                                                                        :</strong>
                                                                                                    <div
                                                                                                        class="d-flex flex-column">
                                                                                                        <% var
                                                                                                            apps=repository.applications.split(',');
                                                                                                            %>
                                                                                                            <% apps.forEach(function(app){
                                                                                                                %>
                                                                                                                <div>

                                                                                                                    <a
                                                                                                                        href="">
                                                                                                                        <span
                                                                                                                            class="badge badge-primary my-2  text-wrap text-start">
                                                                                                                            <%= app
                                                                                                                                %>
                                                                                                                        </span>
                                                                                                                    </a>

                                                                                                                </div>
                                                                                                                <% }) %>
                                                                                                    </div>

                                                                                                </div>
                                                                                                <div class="mb-3">
                                                                                                    <strong
                                                                                                        class="label label-dark">
                                                                                                        Material
                                                                                                        :</strong>
                                                                                                    <div
                                                                                                        class="d-flex flex-column">
                                                                                                        <% var
                                                                                                            materials=repository.materialused.split(',');
                                                                                                            %>
                                                                                                            <% materials.forEach(function(material){
                                                                                                                %>
                                                                                                                <div>

                                                                                                                    <a
                                                                                                                        href="">
                                                                                                                        <span
                                                                                                                            class="badge badge-primary my-2 text-wrap text-start">
                                                                                                                            <%= material
                                                                                                                                %>
                                                                                                                        </span>
                                                                                                                    </a>

                                                                                                                </div>
                                                                                                                <% }) %>
                                                                                                    </div>

                                                                                                </div>
                                                                                                <div class="mb-3">
                                                                                                    <strong
                                                                                                        class="label label-dark">
                                                                                                        Hardware :
                                                                                                    </strong>
                                                                                                    <div
                                                                                                        class="d-flex flex-column">
                                                                                                        <% var
                                                                                                            hardwares=repository.hardwareused.split(',');
                                                                                                            %>
                                                                                                            <% hardwares.forEach(function(hardware){
                                                                                                                %>
                                                                                                                <div>

                                                                                                                    <a
                                                                                                                        href="">
                                                                                                                        <span
                                                                                                                            class="badge badge-primary my-2 text-wrap text-start">
                                                                                                                            <%= hardware
                                                                                                                                %>
                                                                                                                        </span>
                                                                                                                    </a>

                                                                                                                </div>
                                                                                                                <% }) %>
                                                                                                    </div>

                                                                                                </div>

                                                                                                <div class="mb-3">
                                                                                                    <strong
                                                                                                        class="label label-dark">
                                                                                                        Software
                                                                                                        :</strong>
                                                                                                    <div
                                                                                                        class="d-flex flex-column">
                                                                                                        <% var
                                                                                                            softwares=repository.softwareused.split(',');
                                                                                                            %>
                                                                                                            <% softwares.forEach(function(software){
                                                                                                                %>
                                                                                                                <div>

                                                                                                                    <a
                                                                                                                        href="">
                                                                                                                        <span
                                                                                                                            class="badge badge-primary my-2 text-wrap ownbadge text-start">
                                                                                                                            <%= software
                                                                                                                                %>
                                                                                                                        </span>
                                                                                                                    </a>

                                                                                                                </div>
                                                                                                                <% }) %>
                                                                                                    </div>

                                                                                                </div>
                                                                                                <p
                                                                                                    class="card-text my-2 min-height-50">
                                                                                                    <strong>Literature
                                                                                                        Survey
                                                                                                        :</strong>
                                                                                                    <%= repository.literatureSurvey
                                                                                                        %>
                                                                                                </p>
                                                                                                <p
                                                                                                    class="card-text my-2 min-height-50">
                                                                                                    <strong>Methodology
                                                                                                        :</strong>
                                                                                                    <%= repository.methodology
                                                                                                        %>
                                                                                                </p>
                                                                                                <p
                                                                                                    class="card-text my-2 min-height-50">
                                                                                                    <strong>Patent No
                                                                                                        :</strong>
                                                                                                    <%=
                                                                                                        repository.patentInput%>
                                                                                                </p>

                                                                                                <% if
                                                                                                    (repository.linkresearchpaper)
                                                                                                    { %>
                                                                                                    <div
                                                                                                        class="text-center mb-4 ">
                                                                                                        <button
                                                                                                            type="button"
                                                                                                            class="btn btn-primary ">
                                                                                                            <a href="<%= repository.linkresearchpaper %>"
                                                                                                                class="text-light">Link
                                                                                                                Research
                                                                                                                Paper</a>

                                                                                                        </button>
                                                                                                    </div>
                                                                                                    <% } %>

                                                                                                        <p
                                                                                                            class="card-text my-2 min-height-50">
                                                                                                            <strong>Future
                                                                                                                Scope
                                                                                                                :</strong>
                                                                                                            <%= repository.futurescope
                                                                                                                %>
                                                                                                        </p>
                                                                                                        <p
                                                                                                            class="card-text my-2 min-height-50">
                                                                                                            <strong>Conclusion
                                                                                                                :</strong>
                                                                                                            <%= repository.conclusion
                                                                                                                %>
                                                                                                        </p>
                                                                                                        <p
                                                                                                            class="card-text my-2 min-height-50">
                                                                                                            <strong>Testing
                                                                                                                and
                                                                                                                Result
                                                                                                                :</strong>
                                                                                                            <%=
                                                                                                                repository.testing_result%>
                                                                                                        </p>
                                                                                            </div>
                                                                                            </div>

                                                                                    </div>
                                                                                    </div>
                                                                                    </div>
                                                                                </main>


                                                                                <% }) %>









                                                                                    <!-- Fotter -->
                                                                                    <%- include('../Footer/Footer.ejs')
                                                                                        %>


</body>

</html>